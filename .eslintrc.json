{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-alert": "off",
        "react/jsx-props-no-spreading": "off",
        "no-self-assign": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
